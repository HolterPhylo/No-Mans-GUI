<Application x:Class="NoMansGUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Strings="clr-namespace:NoMansGUI.Utils.Localization"
             xmlns:local="clr-namespace:NoMansGUI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:Bootstrapper x:Key="Boostrapper"/>
                </ResourceDictionary>
                <ResourceDictionary Source="./Styles/basic.xaml"/>
                <ResourceDictionary Source="./Styles/MbinTemplates.xaml"/>
                <ResourceDictionary>
                    <Strings:LocalizedStrings x:Key="LocalizedStrings" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
